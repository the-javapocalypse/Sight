{
	"manifest_version": 2,
	"name": "Sight",
	"description": "Replace your New Tab page with an inspirational dashboard",
	"version": "1.0",
	"offline_enabled": true,
	"permissions": [
		"storage",
		"topSites"
	],
	"background": {
		"scripts": [ "background.js" ]
	},
	"icons": {
		"128": "img/128.png"
	},
	"chrome_url_overrides": {
		"newtab": "dashboard.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"web_accessible_resources": [
		"img/128.png"
	]
}